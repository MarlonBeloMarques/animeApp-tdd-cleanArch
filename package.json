{
  "name": "animeapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --passWithNoTests --coverage --no-cache --runInBand --testPathIgnorePatterns=tests/integration",
    "test:staged": "jest --passWithNoTests -- --findRelatedTests --testPathIgnorePatterns=tests/integration",
    "test:integration": "jest --passWithNoTests --no-cache --runInBand --testPathPattern=tests/integration",
    "lint": "eslint '*/**/*.{js,jsx,ts,tsx}' --fix",
    "pretty": "prettier --config ./.prettierrc.js --write \"*/**/*.{js,jsx,ts,tsx,json}\"",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "yarn lint",
      "yarn pretty",
      "yarn test:staged"
    ]
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.16.1",
    "axios": "^0.26.0",
    "react": "17.0.2",
    "react-native": "0.67.2",
    "react-native-svg": "^12.1.1",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^9.0.0",
    "@types/faker": "^5.1.2",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.66.15",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.23",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-root-import": "^6.6.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "faker": "^5.1.0",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^12.3.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.5.1",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
